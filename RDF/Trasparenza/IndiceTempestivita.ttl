@prefix pwod:  <http://nostro_spazio_projectwork_opendata.it/> .
@prefix spcdataCOM:  <http://spcdata.digitpa.gov.it/Comune/> .
@prefix time:    <http://www.w3.org/2006/time#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .

############################################
### Indice di Tempestività dei pagamenti ###
############################################

spcdataCOM:L219 pwod:indiceTemp_trimestre-1-2015 "31"^^xsd:integer .

pwod:t1_2015 a rdf:trimestre;
       time:hasBeginning "2015-01-01"^^xsd:date ;
	     time:hasEnd "2015-03-31"^^xsd:date .
# pwod:t1_2015 owl:sameAs <http://reference.data.gov.uk/doc/gregorian-quarter/2015-Q1>

pwod:indiceTemp_trimestre-1-2015 a rdf:property, pwod:IndiceTempestivita ;
                               pwod:periodoRif pwod:t1-2015 ;
                               rdf:subPropertyOf pwod:IndiceTempestivita .

######## Definizioni principali ########

pwod:IndiceTempestivita a rdf:property, rdf:class ;
			pwod:rifNormativo "Dlgs ...." ;    # Rif. normativo, da studiare
			rdf:label “Indice di tempestività medio dei pagamenti ”^^xsd:string .

pwod:periodoRif a rdf:property ;
                rdf:label “Periodo di riferimento della rilevazione”^^xsd:string .

pwod:trimestre a rdf:class .
# pwod:trimestre owl:sameAs <http://reference.data.gov.uk/def/intervals/Quarter> .